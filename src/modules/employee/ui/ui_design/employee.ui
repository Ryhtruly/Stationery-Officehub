                <string notr="true">/* Tổng thể QWidget với phong cách Glassmorphism */
QWidget {
    background-color: rgba(40, 40, 40, 180); /* Nền đen mờ */
    border: 1px solid rgba(255, 255, 255, 0.2); /* Viền trắng mờ nhẹ */
    border-radius: 10px; /* Bo góc */
    color: #ffffff; /* Chữ trắng */
    font-family: &quot;Arial&quot;, sans-serif;
    font-size: 14px;
}

/* Header giả lập (nếu dùng QLabel làm tiêu đề) */
QLabel.header {
    background-color: rgba(30, 30, 30, 180);
    color: #ffffff;
    padding: 8px;
    border: none;
    border-bottom: 1px solid rgba(255, 255, 255, 0.2);
    font-weight: bold;
}

/* Hover cho các nút hoặc nhãn */
QPushButton:hover, QLabel:hover {
    background-color: rgba(255, 255, 255, 0.1);
    transition: background-color 0.3s ease;
}

/* Hiệu ứng chọn (ví dụ cho QPushButton khi dùng checked hoặc active state) */
QPushButton:checked {
    background-color: rgba(66, 165, 245, 0.4);
    color: #ffffff;
}

/* Thanh cuộn dọc */
QScrollBar:vertical {
    background: rgba(0, 0, 0, 0.3);
    width: 10px;
    border-radius: 5px;
}
QScrollBar::handle:vertical {
    background: qlineargradient(
        x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(66, 165, 245, 0.6),
        stop:1 rgba(30, 136, 229, 0.6)
    );
    border-radius: 5px;
}
QScrollBar::handle:vertical:hover {
    background: qlineargradient(
        x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(100, 181, 246, 0.7),
        stop:1 rgba(66, 165, 245, 0.7)
    );
    transition: background 0.3s ease;
}
QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical,
QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
    background: none;
}

/* Thanh cuộn ngang */
QScrollBar:horizontal {
    background: rgba(0, 0, 0, 0.3);
    height: 10px;
    border-radius: 5px;
}
QScrollBar::handle:horizontal {
    background: qlineargradient(
        x1:0, y1:0, x2:1, y2:0,
        stop:0 rgba(66, 165, 245, 0.6),
        stop:1 rgba(30, 136, 229, 0.6)
    );
    border-radius: 5px;
}
QScrollBar::handle:horizontal:hover {
    background: qlineargradient(
        x1:0, y1:0, x2:1, y2:0,
        stop:0 rgba(100, 181, 246, 0.7),
        stop:1 rgba(66, 165, 245, 0.7)
    );
    transition: background 0.3s ease;
}
QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal,
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
    background: none;
}
